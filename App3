package main;

public class AppGenerics {

	public static void main(String[] args) {
		Box <Integer> ib = new  Box <> ();
		ib.setValue(100);
		Box <Double> db = new  Box<>();
		db.setValue(0.6);
		Box <String> sb = new Box<>();
		sb.setValue("abc");
		
		ib.toString();
		
		
		//System.out.println(ib.getValue());
		//System.out.println(db.getValue());
		//System.out.println(sb.getValue());


	}

}

class Box <T> {
	private T value;
	
Box (T value) {
	this.setValue(value);
}

public Box() {
	
}

public T getValue() {
	return value;
}

public void setValue(T value) {
	this.value = value;
}




public  <T> void toString( ) {
	
	System.out.println(this.getValue());
	    
}
}
